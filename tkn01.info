name = TKN - 01
description = Drupal 7 theme for TKN
core = 7.x

;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;

regions[page_top]            = Page top
regions[page_bottom]         = Page bottom
regions[content]             = Content

regions[header]              = Header
regions[slider]              = Slider
regions[before_content]      = Before content
regions[after_content]       = After content
regions[sidebar_first]       = Sidebar first
regions[footer_firstcolumn]  = Footer first column
regions[footer_secondcolumn] = Footer second column
regions[footer_thirdcolumn]  = Footer third column
regions[footer]              = Footer

;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = a/j/bootstrap/dist/css/bootstrap.min.css
stylesheets[all][] = a/j/slider-revolution/src/css/settings.css
stylesheets[screen, projection][] = a/c/screen.css
stylesheets[print][] = a/c/print.css

;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;

scripts[] = a/j/bootstrap/dist/js/bootstrap.min.js
scripts[] = a/j/slider-revolution/src/js/jquery.themepunch.plugins.min.js
scripts[] = a/j/slider-revolution/src/js/jquery.themepunch.revolution.min.js
scripts[] = a/j/sticky/jquery.sticky.js
scripts[] = a/j/custom.js

;;;;;;;;;;;;;;;;;;;;
;; Settings
;;;;;;;;;;;;;;;;;;;;
settings[google_map_js] = 1
settings[google_map_latitude] = '43.2852543'
settings[google_map_longitude] = '-1.964525'
settings[google_map_zoom] = 16
settings[google_map_canvas] = 'map-canvas'
settings[google_map_js] = 1
